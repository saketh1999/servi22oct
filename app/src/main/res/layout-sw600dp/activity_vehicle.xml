<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">


    <ImageView
        android:id="@+id/vehicle_2wheeler"

        android:layout_width="wrap_content"
        android:layout_height="200dp"
        android:layout_alignLeft="@id/vehicle_ne4"
        android:layout_alignTop="@id/vehicle_ne4"
        android:layout_alignRight="@id/vehicle_ne7"
        android:layout_marginTop="70dp"
        android:layout_marginRight="50dp"
        android:background="@drawable/box_orange" />

    <ImageView
        android:id="@+id/vehicle_4wheeler"

        android:layout_width="wrap_content"
        android:layout_height="200dp"
        android:layout_alignLeft="@id/vehicle_ne7"
        android:layout_alignTop="@id/vehicle_2wheeler"
        android:layout_alignRight="@id/vehicle_ne4"
        android:layout_marginLeft="50dp"
        android:background="@drawable/box_orange" />

    <TextView
        android:id="@+id/vehicle_customer"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@id/vehicle_ne6"
        android:layout_alignParentBottom="true"
        android:layout_marginLeft="190dp"
        android:layout_marginBottom="19dp"
        android:text="customer care"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="20sp" />

    <Spinner
        android:id="@+id/vehicle_spinner"
        android:layout_width="221dp"
        android:layout_height="wrap_content"
        android:layout_alignTop="@id/vehicle_ne10"
        android:layout_alignBottom="@id/vehicle_ne10"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="0dp"
        android:layout_marginRight="-58dp"
        android:layout_marginBottom="0dp"
        android:layout_toLeftOf="@id/vehicle_ne11"
        android:layout_toRightOf="@id/vehicle_ne10"
        android:background="@null" />


    <Button
        android:id="@+id/vehicle_continue"
        android:layout_width="wrap_content"
        android:layout_height="60dp"
        android:layout_alignLeft="@id/vehicle_2wheeler"
        android:layout_alignTop="@id/vehicle_ne10"
        android:layout_alignRight="@id/vehicle_4wheeler"
        android:layout_marginTop="70dp"
        android:background="@drawable/button"
        android:text="Continue"
        android:textAllCaps="false"
        android:textColor="@color/colorPrimary"
        android:textSize="24sp" />

    <ImageView
        android:id="@+id/velicle_ne1"
        android:layout_width="480dp"
        android:layout_height="125dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="35dp"
        app:srcCompat="@drawable/ic_new_servii_logoartboard_3" />

    <View
        android:id="@+id/vehicle_ne2"
        android:layout_width="wrap_content"
        android:layout_height="1dp"
        android:layout_alignTop="@id/velicle_ne1"
        android:layout_marginTop="171dp"
        android:background="@color/dark_grey" />

    <TextView
        android:id="@+id/vehicle_ne3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@id/vehicle_ne2"
        android:layout_alignLeft="@id/velicle_ne1"
        android:layout_alignRight="@id/velicle_ne1"
        android:layout_marginTop="14dp"
        android:text="VEHICLE TYPE"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="25sp" />

    <TextView
        android:id="@+id/vehicle_ne4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@id/vehicle_ne3"
        android:layout_alignTop="@id/vehicle_ne3"
        android:layout_alignRight="@id/vehicle_ne3"
        android:layout_marginTop="40dp"
        android:text="Help us identify your vehicle"
        android:textColor="@color/colorAccent"
        android:textSize="20sp" />

    <View
        android:id="@+id/vehicle_ne5"
        android:layout_width="wrap_content"
        android:layout_height="1dp"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="60dp"
        android:background="@color/colorPrimaryDark" />

    <TextView
        android:id="@+id/vehicle_ne6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="19dp"
        android:text="Need help? Contact                               "
        android:textSize="20sp" />

    <TextView
        android:id="@+id/vehicle_ne7"
        android:layout_width="40dp"
        android:layout_height="wrap_content"
        android:layout_alignTop="@id/vehicle_2wheeler"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="80dp"
        android:gravity="center"
        android:text="@string/or"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="25sp" />

    <TextView
        android:id="@+id/vehicle_ne8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@id/vehicle_2wheeler"
        android:layout_alignTop="@id/vehicle_2wheeler"
        android:layout_alignRight="@id/vehicle_2wheeler"
        android:layout_marginTop="210dp"
        android:gravity="center"
        android:text="2 Wheeler"
        android:textColor="@color/colorAccent"
        android:textSize="22dp" />

    <TextView
        android:id="@+id/vehicle_ne9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@id/vehicle_4wheeler"
        android:layout_alignTop="@id/vehicle_4wheeler"
        android:layout_alignRight="@id/vehicle_4wheeler"
        android:layout_marginTop="210dp"
        android:gravity="center"
        android:text="4 Wheeler"
        android:textColor="@color/colorAccent"
        android:textSize="22dp" />

    <TextView
        android:id="@+id/vehicle_ne10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@id/vehicle_2wheeler"
        android:layout_alignTop="@id/vehicle_2wheeler"
        android:layout_marginLeft="5dp"
        android:layout_marginTop="280dp"
        android:text="    My vehicle is   "
        android:textColor="@color/colorAccent"
        android:textSize="22dp" />

    <TextView
        android:id="@+id/vehicle_ne11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@id/vehicle_4wheeler"
        android:layout_alignTop="@id/vehicle_ne10"
        android:layout_alignRight="@id/vehicle_4wheeler"
        android:layout_marginLeft="60dp"
        android:layout_marginTop="0dp"
        android:text="   in color"
        android:textColor="@color/colorAccent"
        android:textSize="22sp" />

    <View
        android:id="@+id/vehicle_ne12"
        android:layout_width="wrap_content"
        android:layout_height="1dp"
        android:layout_alignTop="@id/vehicle_ne10"
        android:layout_marginTop="30dp"
        android:layout_alignLeft="@id/vehicle_ne10"
        android:layout_alignRight="@id/vehicle_ne11"
        android:background="@color/colorAccent" />

    <ImageView
        android:id="@+id/vehicle_ne13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@id/vehicle_spinner"
        android:layout_alignRight="@id/vehicle_spinner"
        android:layout_alignBottom="@id/vehicle_spinner"
        android:layout_marginTop="3dp"
        android:layout_marginRight="-6dp"
        android:layout_marginBottom="0dp"
        android:background="@drawable/spinner" />

    <ImageView
        android:id="@+id/vehicle_back"
        android:layout_width="70dp"
        android:layout_height="wrap_content"
        android:layout_alignTop="@id/velicle_ne1"
        android:layout_alignBottom="@id/vehicle_ne2"
        android:layout_marginTop="80dp"
        android:layout_marginRight="-40dp"
        android:layout_marginBottom="10dp"
        android:layout_toLeftOf="@id/velicle_ne1"
        app:srcCompat="@drawable/ic_left_arrow" />

    <TextView
        android:id="@+id/vehi_type"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:visibility="gone"/>
</RelativeLayout>